

@model ExamDomain.Model.ClosedQuestion


<h4>Answers</h4>
<hr/>

@Html.ValidationSummary(true, "", new { @class = "text-danger" })

<div class="form-group" id="NumberOfAnswersDiv">
    @Html.LabelFor(model => model.NumberOfAnswers, htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.EditorFor(model => model.NumberOfAnswers, new { htmlAttributes = new { @class = "form-control", id = "NumberOfAnswersTextBox" } })
        @Html.ValidationMessageFor(model => model.NumberOfAnswers, "", new { @class = "text-danger" })
    </div>
</div>

